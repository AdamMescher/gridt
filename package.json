{
  "name": "gridt",
  "version": "0.0.2",
  "description": "Audrey Ford dissertation tool to analyze federal school data set",
  "main": "index.js",
  "engines": {
    "node": "^16.5.0"
  },
  "scripts": {
    "db:seed": "node --max-old-space-size=8192 server/db/seedDb.js",
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'",
    "dev": "nodemon ./server/index.js",
    "heroku:deploy": "node ./server/index.js",
    "clean:server": "rimraf ./server/dist",
    "build:server": "npm run clean:server && babel ./server --out-dir ./server/dist --copy-files",
    "start:server": "nodemon --exec babel-node server/bin/www.js",
    "prod:server": "node ./server/dist/bin/www.js",
    "test": "node --trace-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "npx playwright test && npx playwright show-report",
    "test:e2e-update-snapshots": "npx playwright test --update-snapshots && npx playwright show-report",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier:fe": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:fe": "eslint \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:server": "eslint \"server/**/*.js\"",
    "format:server": "prettier --write \"server/**/*.js\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token=24a0207b229c"
  },
  "keywords": [],
  "author": "Adam Mescher <admescher@gmail.com>",
  "license": "ISC",
